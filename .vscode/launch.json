{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome (Vite)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/apps/client-web",
      "preLaunchTask": "vite:dev"
    },
    {
      "name": "Attach to Chrome (existing)",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}/apps/client-web"
    }
  ]
}
